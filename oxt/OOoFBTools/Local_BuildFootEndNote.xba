			.sFootnotesGeneration		= &quot;Генерация сносок&quot;
			.sHyperLinksGeneration		= &quot;Генерация гиперссылок&quot;
			.sFootnotesGenerationCount	= &quot;Сгенерировано сносок:&quot;
			.sHyperlinksGenerationCount	= &quot;Сгенерировано гиперссылок:&quot;
